{
  "name": "rest-no-auth",
  "version": "1.0.0",
  "description": "This project contains source code and supporting files for a serverless application that you can deploy with the SAM CLI. It includes the following files and folders.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "build": "sam build && sam validate",
    "deploy": "sam deploy --no-confirm-changeset",
    "build_deploy": "npm run build & npm run deploy"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.mjs?(x)",
      "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.310.0",
    "@aws-sdk/lib-dynamodb": "^3.310.0",
    "aws-sdk-client-mock": "^2.1.1",
    "esbuild": "^0.17.16",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.5.0"
  }
}
